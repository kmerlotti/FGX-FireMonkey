  FGX.Helpers in 'FGX.Helpers.pas',
  FGX.ColorsPanel in 'FGX.ColorsPanel.pas',
  FGX.Colors.Presets in 'FGX.Colors.Presets.pas',
  FGX.Types in 'FGX.Types.pas',
